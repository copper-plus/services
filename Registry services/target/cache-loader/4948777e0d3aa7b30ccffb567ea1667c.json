AC;IAEP,MAAM,WAAW,GAAG,GAAG,EAAE;QACvB,KAAK,CAAC,OAAO,CAAC,IAAI,CAAC,WAAW,CAAC,CAAC;IAClC,CAAC,CAAC;IAEF,SAAS,CAAC,GAAG,EAAE;QACb,IAAI,KAAK,CAAC,aAAa,EAAE;YACvB,WAAW,EAAE,CAAC;SACf;IACH,CAAC,EAAE,CAAC,KAAK,CAAC,aAAa,CAAC,CAAC,CAAC;IAE1B,MAAM,aAAa,GAAG,GAAG,EAAE;QACzB,KAAK,CAAC,YAAY,CAAC,KAAK,CAAC,cAAc,CAAC,EAAE,CAAC,CAAC;IAC9C,CAAC,CAAC;IAEF,MAAM,EAAE,cAAc,EAAE,GAAG,KAAK,CAAC;IACjC,OAAO,CACL,oBAAC,KAAK,IAAC,MAAM,QAAC,MAAM,EAAE,WAAW;QAC/B,oBAAC,WAAW,IAAC,MAAM,EAAE,WAAW,+BAAwC;QACxE,oBAAC,SAAS,IAAC,EAAE,EAAC,uCAAuC,qDAA2D;QAChH,oBAAC,WAAW;YACV,oBAAC,MAAM,IAAC,KAAK,EAAC,WAAW,EAAC,OAAO,EAAE,WAAW;gBAC5C,oBAAC,eAAe,IAAC,IAAI,EAAC,KAAK,GAAG;gCAEvB;YACT,oBAAC,MAAM,IAAC,EAAE,EAAC,6BAA6B,EAAC,KAAK,EAAC,QAAQ,EAAC,OAAO,EAAE,aAAa;gBAC5E,oBAAC,eAAe,IAAC,IAAI,EAAC,OAAO,GAAG;gCAEzB,CACG,CACR,CACT,CAAC;AACJ,CAAC,CAAC;AAEF,MAAM,eAAe,GAAG,CAAC,EAAE,QAAQ,EAAc,EAAE,EAAE,CAAC,CAAC;IACrD,cAAc,EAAE,QAAQ,CAAC,MAAM;IAC/B,aAAa,EAAE,QAAQ,CAAC,aAAa;CACtC,CAAC,CAAC;AAEH,MAAM,kBAAkB,GAAG,EAAE,SAAS,EAAE,YAAY,EAAE,CAAC;AAKvD,eAAe,OAAO,CAAC,eAAe,EAAE,kBAAkB,CAAC,CAAC,oBAAoB,CAAC,CAAC","sourcesContent":["import React, { useEffect } from 'react';\nimport { connect } from 'react-redux';\nimport { RouteComponentProps } from 'react-router-dom';\nimport { Modal, ModalHeader, ModalBody, ModalFooter, Button } from 'reactstrap';\nimport { ICrudGetAction, ICrudDeleteAction } from 'react-jhipster';\nimport { FontAwesomeIcon } from '@fortawesome/react-fontawesome';\n\nimport { IDelivery } from 'app/shared/model/delivery.model';\nimport { IRootState } from 'app/shared/reducers';\nimport { getEntity, deleteEntity } from './delivery.reducer';\n\nexport interface IDeliveryDeleteDialogProps extends StateProps, DispatchProps, RouteComponentProps<{ id: string }> {}\n\nexport const DeliveryDeleteDialog = (props: IDeliveryDeleteDialogProps) => {\n  useEffect(() => {\n    props.getEntity(props.match.params.id);\n  }, []);\n\n  const handleClose = () => {\n    props.history.push('/delivery');\n  };\n\n  useEffect(() => {\n    if (props.updateSuccess) {\n      handleClose();\n    }\n  }, [props.updateSuccess]);\n\n  const confirmDelete = () => {\n    props.deleteEntity(props.deliveryEntity.id);\n  };\n\n  const { deliveryEntity } = props;\n  return (\n    <Modal isOpen toggle={handleClose}>\n      <ModalHeader toggle={handleClose}>Confirm delete operation</ModalHeader>\n      <ModalBody id=\"cuserviceApp.delivery.delete.question\">Are you sure you want to delete this Delivery?</ModalBody>\n      <ModalFooter>\n        <Button color=\"secondary\" onClick={handleClose}>\n          <FontAwesomeIcon icon=\"ban\" />\n          &nbsp; Cancel\n        </Button>\n        <Button id=\"jhi-confirm-delete-delivery\" color=\"danger\" onClick={confirmDelete}>\n          <FontAwesomeIcon icon=\"trash\" />\n          &nbsp; Delete\n        </Button>\n      </ModalFooter>\n    </Modal>\n  );\n};\n\nconst mapStateToProps = ({ delivery }: IRootState) => ({\n  deliveryEntity: delivery.entity,\n  updateSuccess: delivery.updateSuccess,\n});\n\nconst mapDispatchToProps = { getEntity, deleteEntity };\n\ntype StateProps = ReturnType<typeof mapStateToProps>;\ntype DispatchProps = typeof mapDispatchToProps;\n\nexport default connect(mapStateToProps, mapDispatchToProps)(DeliveryDeleteDialog);\n"]}]}